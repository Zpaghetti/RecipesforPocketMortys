<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zawss.android.rpm2">
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/icon4"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"> <!-- This meta-data tag is required to use Google Play Services. -->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zawss.android.rpm2.attacks" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name="com.zawss.android.rpm2.Mortys" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name="com.zawss.android.rpm2.Quests" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name="com.zawss.android.rpm2.Recipes" android:theme="@style/Theme.AppCompat.NoActionBar"/>



        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />

    </application>


</manifest>